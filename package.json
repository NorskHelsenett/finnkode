{
  "name": "finnkode",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ehelse/finnkode.git",
  "author": "Steffen Lien <steffen.lien@ehelse.no>",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && concurrently -k \"yarn sass\" \"yarn webpack:watch\" \"bundle exec guard\"",
    "clean": "rimraf dist",
    "jekyll:serve": "bundle exec jekyll serve --watch",
    "build": "rimraf dist && webpack && bundle exec jekyll build",
    "webpack:watch": "webpack --watch  --progress -d",
    "sass": "sass -I src/scss --watch src/scss/master.scss:dist/css/main.css"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "expose-loader": "^0.7.5",
    "livereload-js": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "guard": "^0.1.2",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1"
  }
}
